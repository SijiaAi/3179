{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": "Immigrants in Australia(2021)",
    "width": 800,
    "height": 400,
    "projection": {"type": "equalEarth"},
    "data": {
      "url": "/Homework9/js/ne_10m-1.json",
      "format": {"type": "topojson", "feature": "ne_10m_admin_1_states_provinces"}
    },
    "transform": [
      {
        "lookup": "properties.name",
        "from": {
          "data": {
            "url": "/Homework9/data/immigrants.csv"
          },
          "key": "State",
          "fields": ["immigrants"]
        }
      },
      {"calculate": "datum.immigrants + 0.1", "as": "Total Immigrants"}
    ],
    "mark": {"type": "geoshape"},
    "encoding": {
      "color": {
        "field": "Total Immigrants",
        "type": "quantitative",
        "scale": {"type": "log"}
      },
      "tooltip": [
        {"field": "properties.name", "type": "nominal", "title": "State"},
        {"field": "immigrants", "type": "quantitative"}
      ]
    }
  }